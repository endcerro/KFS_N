{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "gdb",
        "request": "attach",
        "name": "Attach to QEMU",
        "executable": "${workspaceRoot}/isofiles/boot/kernel.bin",
        "target": ":1234",
        "remote": true,
        "cwd": "${workspaceRoot}",
        "valuesFormatting": "parseText",
        "gdbpath": "gdb",
        "preLaunchTask": "Run QEMU"
      }
    ]
  }